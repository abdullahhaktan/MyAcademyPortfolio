@model SocialMedia

@{
    ViewData["Title"] = "Sosyal medya ekleme";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">
    <div class="form-group">
        <label>Sosyal medya(instagram,X,github etc.)</label>
        <input type="text" class="form-control" asp-for="Name" />
        <span class="text-danger" asp-validation-for="Name"></span>
    </div>

    <div class="form-group">
        <label>Url</label>
        <input type="text" class="form-control" asp-for="Url" />
        <span class="text-danger" asp-validation-for="Url"></span>
    </div>
    <button class="btn btn-outline-primary">Kaydet</button>

</form>