@model Skill

@{
    ViewData["Title"] = "Yetenek Ekle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form action="/Skill/CreateSkill/" method="post">

    <div class="form-group">
        <label>Yetenek başlığı</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>


    <div class="form-group">
        <label>Yüzde</label>
        <input type="text" class="form-control" asp-for="Percentage" />
    </div>

    <button type="submit" class="btn btn-outline-primary">Kaydet</button>

</form>