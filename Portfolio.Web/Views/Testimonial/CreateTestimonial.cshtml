@model Testimonial

@{
    ViewData["Title"] = "referans oluştur";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">

    <div class="form-group">
        <label>Kim</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>

    <div class="form-group">
        <label>Başlık</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>

    <div class="form-group">
        <label>Resim</label>
        <input type="text" class="form-control" asp-for="ImageUrl" />
    </div>

    <div class="form-group">
        <label>Yorum</label>
        <input type="text" class="form-control" asp-for="Comment" />
    </div>

    <div class="form-group">
        <label>Yıldız</label>
        <input type="text" class="form-control" asp-for="Review" />
    </div>

    <button type="submit" class="btn btn-outline-primary">Kaydet</button>

</form>