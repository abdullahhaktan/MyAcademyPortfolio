@model Experience

@{
    ViewData["Title"] = "Deneyim güncelle";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">

    <div class="form-group">
        <label>Başlık</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>

    <div class="form-group">
        <label>Başlangıç Yılı</label>
        <input type="text" class="form-control" asp-for="StartYear" />
    </div>

    <div class="form-group">
        <label>Bitişi Yılı</label>
        <input type="text" class="form-control" asp-for="EndYear" />
    </div>

    <div class="form-group">
        <label>Şirket</label>
        <input type="text" class="form-control" asp-for="Company" />
    </div>

    <div class="form-group">
        <label>Şehir</label>
        <input type="text" class="form-control" asp-for="City" />
    </div>

    <div class="form-group">
        <label>Açıklama</label>
        <input type="text" class="form-control" asp-for="Description" />
    </div>

    <button type="submit" class="btn btn-outline-primary">Kaydet</button>

</form>